            ],
            "styles": [
              "src/app/styles/styles.scss",
              "projects/prime-registration/src/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/styles"
              ]
            },
            "preserveSymlinks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "projects/prime-registration/src/environments/environment.ts",
                "with": "projects/prime-registration/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
