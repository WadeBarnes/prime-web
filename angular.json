{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Prime": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Prime",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./assets",
                "output": "./assets"
              }
            ],
            "styles": [
              "src/app/styles/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/styles"
              ]
            },
            "preserveSymlinks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Prime:build",
            "port": 4305
          },
          "configurations": {
            "production": {
              "browserTarget": "Prime:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Prime:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/app/styles/styles.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/styles"
              ]
            },
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "Prime-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "webdriverUpdate": false,
            "devServerTarget": "Prime:serve",
            "port": 4306
          },
          "configurations": {
            "production": {
              "devServerTarget": "Prime:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "prime-enrollment": {
      "root": "projects/prime-enrollment/",
      "sourceRoot": "projects/prime-enrollment/src",
      "projectType": "application",
      "prefix": "enroll",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/prime-enrollment",
            "index": "projects/prime-enrollment/src/index.html",
            "main": "projects/prime-enrollment/src/main.ts",
            "polyfills": "projects/prime-enrollment/src/polyfills.ts",
            "tsConfig": "projects/prime-enrollment/src/tsconfig.app.json",
            "assets": [
              "projects/prime-enrollment/src/favicon.ico",
              "projects/prime-enrollment/src/assets",
              {
                "glob": "**/*",
                "input": "./assets",
                "output": "./assets"
              }
            ],
            "styles": [
              "src/app/styles/styles.scss",
              "projects/prime-enrollment/src/styles.scss",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/app/styles"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "projects/prime-enrollment/src/environments/environment.ts",
                "with": "projects/prime-enrollment/src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "prime-enrollment:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "prime-enrollment:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "prime-enrollment:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/prime-enrollment/src/test.ts",
            "polyfills": "projects/prime-enrollment/src/polyfills.ts",
            "tsConfig": "projects/prime-enrollment/src/tsconfig.spec.json",
            "karmaConfig": "projects/prime-enrollment/karma.conf.js",
            "styles": [
              "projects/prime-enrollment/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/prime-enrollment/src/favicon.ico",
              "projects/prime-enrollment/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/prime-enrollment/tsconfig.app.json",
              "projects/prime-enrollment/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "prime-enrollment-e2e": {
      "root": "projects/prime-enrollment-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/prime-enrollment-e2e/protractor.conf.js",
            "devServerTarget": "prime-enrollment:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "prime-enrollment:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/prime-enrollment-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "prime-registration": {
